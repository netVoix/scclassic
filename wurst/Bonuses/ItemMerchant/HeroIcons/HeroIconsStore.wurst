public function getHero(int rid, int index) returns heroMap
  switch rid
    case MAIN_CASTLE_BLOOD_ELF_ID
      return HEROES_BLOOD_ELF[index]
    case MAIN_CASTLE_DEMON_ID
      return HEROES_DEMON[index]
    case MAIN_CASTLE_DWARF_ID
      return HEROES_DWARF[index]
    case MAIN_CASTLE_FEL_ORC_ID
      return HEROES_FEL_ORC[index]
    case MAIN_CASTLE_GOBLIN_ID
      return HEROES_GOBLIN[index]
    case MAIN_CASTLE_HUMAN_ID
      return HEROES_HUMAN[index]
    case MAIN_CASTLE_NAGA_ID
      return HEROES_NAGA[index]
    case MAIN_CASTLE_NIGHT_ELD_ID
      return HEROES_NIGHT_ELF[index]
    case MAIN_CASTLE_ORC_ID
      return HEROES_ORC[index]
    case CORRUPTED_TREE_OF_LIFE_LV1
      return HEROES_SATYR[index]
    case MAIN_CASTLE_TAUREN_ID
      return HEROES_TAUREN[index]
    case MAIN_CASTLE_TROLL_ID
      return HEROES_TROLL[index]
    case MAIN_CASTLE_UNDEAD_ID
      return HEROES_UNDEAD[index]
    case MAIN_CASTLE_ROGUE_LVL1
      return HEROES_ROGUE[index]     
    default
      return heroMap(0, 0, 0)