set S0N=ITEM_TYPE_ARTIFACT
set TRN=SIN
endif
endif
set SIN=SIN+1
exitwhen SIN>$A
endloop
if(TPN==0)then
set S0N=null
return
endif
call Q4N(S0N,TRN)
set S0N=null
endfunction
