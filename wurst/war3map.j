set S0N=ITEM_TYPE_PERMANENT
set TRN=SIN
endif
endif
if(bj_stockAllowedCharged[SIN])then
set TPN=TPN+1
if(GetRandomInt(1,TPN)==1)then
set S0N=ITEM_TYPE_CHARGED
set TRN=SIN
