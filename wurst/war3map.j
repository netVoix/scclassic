function IAN takes player NON returns boolean
return(GetPlayerSlotState(NON)==PLAYER_SLOT_STATE_PLAYING)and(GetPlayerController(NON)==MAP_CONTROL_USER)
endfunction
function NFN takes nothing returns nothing
local player array NKN
local integer NUN=0
local integer BON=0
loop
if IAN(Player(BON))then
set NKN[NUN]=Player(BON)
set NUN=NUN+1
endif
set BON=BON+1
exitwhen BON>$B
endloop
if NUN==0 then
set PSV=null
set PUV=null
elseif NUN==1 then
set PSV=NKN[0]
set PUV=null
else
set NUN=NUN-1
set BON=GetRandomInt(0,NUN)
set PSV=NKN[BON]
set NKN[BON]=NKN[NUN]
set NUN=NUN-1
set BON=GetRandomInt(0,NUN)
set PUV=NKN[BON]
endif
endfunction
